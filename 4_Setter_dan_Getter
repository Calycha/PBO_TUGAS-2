using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _4_Setter_dan_Getter
{
    class User
    {
        static void Main(string[] args)
        {
            user calycha = new user();
            calycha.Username = "calycha2003";
            calycha.Password = "belajarbahasac#";

            Console.WriteLine("Username = " + calycha.Username);
            Console.WriteLine("Password = " + calycha.Password);

            Console.ReadKey();
        }
    }
    public class user
    {
        string username;
        string password;
        public string Username
        {
            get { return username; }
            set { username = value; }
        }

        public string Password
        {
            get { return password; }
            set { password = value; }
        }
    }
}
